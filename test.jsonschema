{
  "$schema": "http://json-schema.org/draft-07/schema",
  "description": "Object for saving correction data associated with LabTools",
  "type": "object",
  "name": "FieldTest",
  "properties": {
    "ltid": {
      "type": "string"
    },
    "athlete": {
      "type": "string"
    },
    "field_analysis": {
      "type": "string"
    },

    "activityFiles": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "originalFilename": { "type": "string" },
          "id": { "type": "string" }
        }
      }
    },

    "research_release": {
      "type": "boolean"
    },
    "weight": {
      "type": "number"
    },
    "height": {
      "type": "number"
    },
    "location": {
      "type": "string"
    },
    "temperature": {
      "type": "number"
    },
    "date": {
      "type": "string"
    },
    "powermeter": {
      "type": "string"
    },

    "cp_regression": {
      "type": "object",
      "properties": {
        "cp10s": {
          "type": "number"
        },
        "cp30s": {
          "type": "number"
        },
        "cp3": {
          "type": "number"
        },
        "cp5": {
          "type": "number"
        },
        "cp10": {
          "type": "number"
        },
        "corrections": {
          "type": "object",
          "properties": {
            "cp10s": {
              "type": "number"
            },
            "cp30s": {
              "type": "number"
            },
            "cp3": {
              "type": "number"
            },
            "cp5": {
              "type": "number"
            },
            "cp10": {
              "type": "number"
            }
          }
        },
        "slope": {
          "type": "number"
        },
        "determination": {
          "type": "number"
        },
        "w_prime": {
          "type": "number"
        }
      }
    }
  }
}
