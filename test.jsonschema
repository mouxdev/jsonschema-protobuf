{
    "$schema": "http://json-schema.org/draft-07/schema",
    "description": "Object for saving athlete data associated with LabTools",
    "type": "object",
    "name": "Athlete",
    "properties": {
        "name_given": {
            "type": "string"
        },
        "tests": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "discipline": {
            "enum": ["racing", "mtbxco", "mtbdh", "triathlon", "cross", "gravel", "other"]
        }
    }
}